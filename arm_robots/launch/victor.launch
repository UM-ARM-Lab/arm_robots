<launch>
    <arg name="moveit_controller_manager" default="fake"/>
    <arg name="use_victor" default="false"/>
    <arg name="use_val" default="false"/>

    <include file="$(find victor_moveit_config)/launch/move_group.launch">
        <arg name="allow_trajectory_execution" value="true"/>
        <arg name="moveit_controller_manager" value="$(arg moveit_controller_manager)"/>
    </include>
    <include file="$(find arm_robots)/launch/trajectory_follower.launch">
        <arg name="use_victor" value="$(arg use_victor)"/>
    </include>
    <node pkg="jacobian_follower" type="jacobian_follower" name="jacobian_follower">
        <param name="traj_name" value="/victor/both_arms_trajectory_controller/follow_joint_trajectory/"/>
    </node>
</launch>